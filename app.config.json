{
  "expo": {
    "name": "Predalgo",
    "slug": "Predalgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lusersd.Predalgo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lusersd.Predalgo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "758f7ef7-14d1-49ea-8306-e527c93c1f17"
      }
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Picalgo needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "Picalgo needs access to your Microphone."
        }
      ],
      "react-native-compressor",
      [
        "expo-build-properties",
        {
          "ios": {
            "flipper": true
          }
        }
      ],
      [
        "react-native-admob-native-ads",
        {
          "androidAppId1":  "ca-app-pub-3940256099942544/1044960115",
          "iosAppId1":  "ca-app-pub-3940256099942544/2521693316",
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511",
          "facebookMediation": false
        }
      ]
    ]
  }
}
